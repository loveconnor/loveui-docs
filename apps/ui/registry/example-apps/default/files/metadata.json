{
  "title": "Documentary Asset Vault",
  "description": "A production asset manager with storage insights, collection navigation, and crew activity.",
  "category": "productivity",
  "tags": ["assets", "documentary", "storage", "collections"],
  "isNew": true,
  "files": [
    {
      "path": "app/(files)/page.tsx",
      "type": "page"
    },
    {
      "path": "app/(files)/layout.tsx",
      "type": "layout"
    },
    {
      "path": "components/files/sidebar.tsx",
      "type": "component"
    },
    {
      "path": "components/files/header.tsx",
      "type": "component"
    },
    {
      "path": "components/files/content.tsx",
      "type": "component"
    },
    {
      "path": "store/files-store.ts",
      "type": "lib"
    }
  ],
  "routes": [
    {
      "path": "/",
      "title": "All Assets",
      "description": "Main production asset explorer"
    },
    {
      "path": "/recent",
      "title": "Latest",
      "description": "Recently accessed assets"
    },
    {
      "path": "/shared",
      "title": "Shared",
      "description": "Shared assets and collections"
    },
    {
      "path": "/starred",
      "title": "Pinned",
      "description": "Pinned assets"
    },
    {
      "path": "/trash",
      "title": "Archive Bin",
      "description": "Removed assets"
    },
    {
      "path": "/folder/[id]",
      "title": "Collection",
      "description": "Dynamic collection view"
    }
  ]
}
