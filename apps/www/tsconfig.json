{
  "extends": "@loveui/typescript-config/nextjs.json",
  "compilerOptions": {
    "plugins": [
      {
        "name": "next"
      }
    ],
    "typeRoots": [
      "./node_modules/@types",
      "../node_modules/@types",
      "../../node_modules/@types",
      "../../node_modules/.bun/@types+three@0.170.0/node_modules/@types",
      "./types"
    ],
    "paths": {
      "@/*": ["./*"],
      "three": [
        "../../node_modules/.bun/@types+three@0.170.0/node_modules/@types/three"
      ],
      "three/*": [
        "../../node_modules/.bun/@types+three@0.170.0/node_modules/@types/three/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "next-env.d.ts",
    "next.config.ts",
    ".next/types/**/*.ts"
  ],
  "exclude": ["node_modules"]
}
