{
  "extends": "@loveui/typescript-config/nextjs.json",
  "compilerOptions": {
    "declaration": false,
    "declarationMap": false,
    "esModuleInterop": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "@/ui/*": [
        "./registry/default/ui/*"
      ],
      "@/motion-primitives/*": [
        "./registry/default/motion-primitives/*"
      ],
      "@loveui/*": [
        "../../packages/*"
      ],
      "@source/*": [
        "./.source/*"
      ],
      "@loveui/ui/*": [
        "../../packages/ui/src/*"
      ],
      "three": [
        "../../node_modules/.bun/@types+three@0.170.0/node_modules/@types/three"
      ],
      "three/*": [
        "../../node_modules/.bun/@types+three@0.170.0/node_modules/@types/three/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "next.config.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
