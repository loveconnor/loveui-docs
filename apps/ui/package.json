{
  "name": "ui",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "next dev --port 4000",
    "build": "next build",
    "start": "next start --port 4000",
    "lint": "next lint --max-warnings 0",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "registry:build": "tsx --tsconfig ./tsconfig.scripts.json ./scripts/build-registry.mts && prettier --log-level silent --write \"registry/**/*.{ts,tsx,json,mdx}\" --cache",
    "ui:propagate": "tsx --tsconfig ./tsconfig.scripts.json ./scripts/propagate-ui.mts",
    "postinstall": "fumadocs-mdx",
    "clean": "rm -rf node_modules && rm -rf .turbo && rm -rf .next && rm -rf .source"
  },
  "dependencies": {
    "@base-ui-components/react": "1.0.0-beta.4",
    "@faker-js/faker": "^10.1.0",
    "@hugeicons/core-free-icons": "^1.0.16",
    "@hugeicons/react": "^1.0.5",
    "@loveui/ether": "workspace:*",
    "@loveui/gradiant-blinds": "workspace:*",
    "@loveui/gradient-mesh": "workspace:*",
    "@loveui/shader-ripple": "workspace:*",
    "@loveui/silk": "workspace:*",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-three/fiber": "^9.0.0",
    "@remixicon/react": "^4.6.0",
    "@loveui/ui": "workspace:*",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.1.1",
    "fumadocs-core": "^16.0.2",
    "fumadocs-mdx": "^13.0.0",
    "jotai": "^2.14.0",
    "lodash": "^4.17.21",
    "lodash.groupby": "^4.6.0",
    "lucide-react": "^0.543.0",
    "next": "^16.0.0",
    "ogl": "^1.0.6",
    "radix-ui": "^1.4.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-resizable-panels": "^3.0.6",
    "shadcn": "^3.2.1",
    "tailwind-merge": "^3.3.1",
    "three": "^0.181.0",
    "ts-morph": "^26.0.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.6.1",
    "@shikijs/transformers": "^3.12.2",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/mdx": "^2.0.13",
    "@types/three": "^0.170.0",
    "@types/node": "^24.3.1",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@loveui/eslint-config": "workspace:^",
    "@loveui/typescript-config": "workspace:*",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "rehype-pretty-code": "^0.14.1",
    "rimraf": "^6.0.1",
    "shiki": "^3.12.2",
    "tailwindcss": "^4.1.13",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "prettier": {
    "endOfLine": "lf",
    "semi": false,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "es5",
    "importOrder": [
      "^(react/(.*)$)|^(react$)",
      "^(next/(.*)$)|^(next$)",
      "<THIRD_PARTY_MODULES>",
      "",
      "^@loveui/(.*)$",
      "",
      "^types$",
      "^@/types/(.*)$",
      "^@/config/(.*)$",
      "^@/lib/(.*)$",
      "^@/hooks/(.*)$",
      "^@/components/ui/(.*)$",
      "^@/components/(.*)$",
      "^@/registry/(.*)$",
      "^@/styles/(.*)$",
      "^@/app/(.*)$",
      "^@/www/(.*)$",
      "",
      "^[./]"
    ],
    "importOrderParserPlugins": [
      "typescript",
      "jsx",
      "decorators-legacy"
    ],
    "plugins": [
      "@ianvs/prettier-plugin-sort-imports",
      "prettier-plugin-tailwindcss"
    ],
    "tailwindFunctions": [
      "cva",
      "cn",
      "clsx"
    ],
    "tailwindStylesheet": "./app/globals.css"
  }
}
